{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron-main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "wait-on http://localhost:5180 && electron .",
    "start": "concurrently \"npm run dev\" \"npm run electron\"",
    "electron:build": "npm run build && npx electron-builder",
    "electron:build:linux": "npm run build && npx electron-builder --linux"
  },
  "build": {
    "appId": "com.antigravity.gestao-dti",
    "productName": "Painel Central de Operações de TI",
    "directories": {
      "output": "dist-electron"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility"
    },
    "files": [
      "dist/**/*",
      "electron-main.cjs",
      "public/**/*",
      "package.json"
    ],
    "asar": true
  },
  "dependencies": {
    "axios": "^1.13.5",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.575.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "electron-is-dev": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.3.1",
    "wait-on": "^9.0.4"
  }
}